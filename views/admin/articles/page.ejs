<%- include ("../../partials/header.ejs") %>
<%- include ("../../partials/homenavbar.ejs") %>
<body>
	<div class="container">		
		<hr>
		<% result.articles.rows.forEach(article => { %>
			<div class="card">
				<div class="card-header">
					<h6><%= article.title %></h6>
				</div>
				<div class="card-body">
					<a class="btn btn-primary" href="/<%= article.slug %>">Acessar artigo</a>
				</div>
			</div>
			<br>
		<% }) %>
	</div>
</body>

